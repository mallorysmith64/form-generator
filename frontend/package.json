{
  "name": "frontend",
  "private": true,
  "proxy": "http://localhost:5000",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others 'yarn run vite' 'yarn run tsc-watch' 'yarn run start-backend'",
    "start-backend": "cd ../backend && FLASK_ENV=development FLASK_DEBUG=1 env/bin/flask run",
    "tsc-watch": "tsc-watch --onFailure 'play /snap/gtk-common-themes/1535/share/sounds/freedesktop/stereo/window-question.oga'",
    "vite": "ulimit -Sn 10000 && vite serve   --host 0.0.0.0 --port 8080 --mode development --clearScreen false",
    "preview": "ulimit -Sn 10000 && vite preview --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "tsc": "tsc"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.3.0",
    "concurrently": "^7.6.0",
    "immutability-helper": "^3.1.1",
    "nodemon": "^2.0.20",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-config": "^5.1.1",
    "tsc-watch": "^6.0.0",
    "usehooks-ts": "^2.9.1",
    "vite": "^4.1.1",
    "vite-plugin-eslint": "^1.8.1",
    "watch": "^1.0.2"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.10",
    "@types/react-router-config": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.0",
    "bulma": "^0.9.4",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "prettier": "^2.8.3",
    "react-router-dom": "^6.7.0",
    "typescript": "^4.9.4",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix",
      "npx prettier --write"
    ]
  },
  "main": "index.js",
  "repository": "git@github.com:mallorysmith64/form-generator-1.0.git",
  "license": "MIT"
}
